<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.s2start.matheus.fiveconn">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.FiveConnApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activities.CadastroActivity" />
        <activity android:name=".ui.activities.MainActivity" />
        <activity android:name=".ui.activities.PerfilActivity" />
        <activity android:name=".ui.activities.NewPostActivity" />
        <activity android:name=".ui.activities.ConfiguracaoActivity" />
        <activity android:name=".ui.activities.DetalhePostActivity"></activity>
    </application>

</manifest>